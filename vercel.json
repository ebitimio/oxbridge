{
  "version": 2,
  "name": "oxbridge-lms",
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/",
      "destination": "/indexauthorization.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}